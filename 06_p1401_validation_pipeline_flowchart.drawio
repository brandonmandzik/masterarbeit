        <mxCell id="output-rmse" value="rmse_scores.csv&#xa;&#xa;metric, RMSE, RMSE_star, RMSE_LOOCV&#xa;COVER_overall, 0.34, 0.18, 0.41&#xa;COVER_semantic, 0.56, 0.42, 0.63&#xa;SI, 0.72, 0.58, 0.79&#xa;TI, 0.68, 0.51, 0.74&#xa;..." style="shape=document;whiteSpace=wrap;html=1;boundedLbl=1;fillColor=#dae8fc;strokeColor=#6c8ebf;fontSize=9;align=left;fontFamily=Courier New" vertex="1" parent="1">
