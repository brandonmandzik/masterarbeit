        <mxCell id="output-plots" value="Scatter Plots&#xa;&#xa;scatter_plots.png&#xa;&#xa;• MOS vs Metric (raw data)&#xa;• Fitted polynomial curve&#xa;• Confidence intervals&#xa;• R² values&#xa;&#xa;One plot per metric" style="shape=document;whiteSpace=wrap;html=1;boundedLbl=1;fillColor=#dae8fc;strokeColor=#6c8ebf;fontSize=9;align=left" vertex="1" parent="1">
          <mxGeometry x="80" y="1290" width="240" height="140" as="geometry" />
        </mxCell>

        <mxCell id="output-loocv" value="loocv_predictions.csv&#xa;&#xa;video_name, MOS_actual, MOS_predicted, error&#xa;source1_1.mp4, 4.21, 4.15, 0.06&#xa;source1_2.mp4, 3.87, 3.92, -0.05&#xa;source2_1.mp4, 4.56, 4.48, 0.08&#xa;..." style="shape=document;whiteSpace=wrap;html=1;boundedLbl=1;fillColor=#dae8fc;strokeColor=#6c8ebf;fontSize=9;align=left;fontFamily=Courier New" vertex="1" parent="1">
          <mxGeometry x="360" y="1250" width="240" height="120" as="geometry" />
        </mxCell>

        <mxCell id="output-summary" value="Summary Report&#xa;&#xa;Best Performing Metric:&#xa;  COVER_overall&#xa;  • Pearson: 0.89 (Excellent)&#xa;  • Spearman: 0.92 (Excellent)&#xa;  • RMSE: 0.34&#xa;  • RMSE*: 0.18&#xa;  • LOOCV RMSE: 0.41&#xa;&#xa;Conclusion:&#xa;  COVER overall score strongly predicts&#xa;  human perception (MOS) for AI-generated&#xa;  videos in this dataset." style="rounded=0;whiteSpace=wrap;html=1;fillColor=#e1d5e7;strokeColor=#9673a6;fontSize=9;align=left;fontStyle=1" vertex="1" parent="1">
          <mxGeometry x="80" y="1390" width="520" height="180" as="geometry" />
        </mxCell>

